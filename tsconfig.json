{
  "compilerOptions": {
    "target": "es2015",
    "declaration": true,
    "module": "commonjs",
    "sourceMap": false,
    "outDir": "./dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "skipLibCheck": true,
    "types": [
      "node"
    ]
  }
}
